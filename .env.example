# Database Configuration
DB_NAME=google_play_analysis
DB_USER=your_username
DB_PASSWORD=your_password
DB_HOST=localhost
DB_PORT=5432

# AI Analysis Configuration
OPENAI_API_KEY=your_openai_api_key_here

